    # mct "One."
    mct "Erstens."

# game/code/e03/e03s02.rpy:45
translate deutsch e03s02_1cd8aad7:

    # mct "And two."
    mct "Und zweitens."

# game/code/e03/e03s02.rpy:53
translate deutsch e03s02_59dcd104:

    # mh "Mmmhawa."
    mh "Mmmhawa."

# game/code/e03/e03s02.rpy:60
translate deutsch e03s02_bc9f4b26:

    # mh "Hey there."
    mh "Hallo zusammen."

# game/code/e03/e03s02.rpy:63
translate deutsch e03s02_8ddcc085:

    # mc "Good morning."
    mc "Guten Morgen."

# game/code/e03/e03s02.rpy:69
